{
  "name": "@alcalzone/release-script-core",
  "publishConfig": {
    "access": "public"
  },
  "version": "3.0.0-alpha.0",
  "description": "Al Calzone's release script: Core components",
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "AlCalzone",
    "email": "d.griesel@gmx.net"
  },
  "main": "build/index.js",
  "exports": {
    ".": "./build/index.js",
    "./package.json": "./package.json",
    "./*.map": "./build/*.js.map",
    "./*": "./build/*.js"
  },
  "types": "build/index.d.ts",
  "typesVersions": {
    "*": {
      "build/index.d.ts": [
        "build/index.d.ts"
      ],
      "*": [
        "build/*"
      ]
    }
  },
  "files": [
    "build/",
    "LICENSE"
  ],
  "engines": {
    "node": ">=12.20"
  },
  "dependencies": {
    "execa": "^5.1.1"
  },
  "devDependencies": {
    "colors": "^1.4.0"
  },
  "scripts": {
    "clean": "tsc -b tsconfig.build.json --clean",
    "prebuild": "rimraf ./build",
    "build": "tsc -p tsconfig.build.json",
    "watch": "tsc -p tsconfig.build.json --watch"
  },
  "homepage": "https://github.com/AlCalzone/release-script/tree/main/packages/core#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:AlCalzone/release-script.git"
  },
  "bugs": {
    "url": "https://github.com/AlCalzone/release-script/issues"
  },
  "readmeFilename": "README.md"
}
